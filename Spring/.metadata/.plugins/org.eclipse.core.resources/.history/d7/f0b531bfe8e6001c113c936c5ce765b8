package com.test01;

public class MesaageBeanimpl implements MessageBean {
	private String fruit;
	private int cost;
	@Override
	public void sayHello(String name) {
		System.out.println("과일: "+fruit +"\t 가격: "+cost);
		
	}
	
	public MesaageBeanimpl() {
		this.fruit = "banana";
		this.cost =5000;
	}
	
	public MesaageBeanimpl(String fruit,int cost) {
		this.fruit = fruit;
		this.cost = cost;
	}
	
}
