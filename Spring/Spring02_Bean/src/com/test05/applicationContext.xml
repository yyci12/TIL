<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="hong" class="com.test05.Developer">
		<property name="dept" value="개발"></property>
		<property name="name" value="홍길동"></property>
		<property name="salary" value="2000000"></property>
	</bean>
	
	<bean id="lee" class="com.test05.Developer">
		<constructor-arg name="name" value="이순신"></constructor-arg>
		<constructor-arg name="salary" value="30000000"></constructor-arg>
		<property name="dept" value="개발"></property>
	</bean>
</beans>
